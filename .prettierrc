{
  "semi": false,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "endOfLine": "crlf",
  "importOrder": [
    "^react-ace",
    "^ace-builds/src-noconflict/mode-javascript",
    "^ace-builds/src-noconflict/theme-solarized_dark",
    "^ace-builds/src-noconflict/theme-solarized_light",
    "^(@clients).*",
    "^(@components).*",
    "^(@componentsUsingStore).*",
    "^(@store).*",
    "^(@common|@contexts|@hooks|@utils|@icons).*",
    "^.*/services/(.*)$",
    "^(..\\/|.\\/)[^@].*(?<!css)$",
    "^.*.(module).css$"
  ],
  "importOrderSeparation": true
}

